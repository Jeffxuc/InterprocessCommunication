<Window x:Class="AnonymousPipe_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnonymousPipe_Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <Label Content="Get Data from update exe" HorizontalAlignment="Center" FontSize="20"/>
        <Label x:Name="getTxt" Margin="10" HorizontalAlignment="Center" FontSize="20"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Label x:Name="readPipeData" FontSize="20" Foreground="HotPink"/>
            <Label  HorizontalAlignment="Center" FontSize="20" Content="%" Foreground="HotPink"/>
        </StackPanel>
        <Label x:Name="resTxt" HorizontalAlignment="Center" Margin="10" FontSize="30"/>
    </StackPanel>
</Window>
